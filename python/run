test=test_number_to_compound_fraction.py
golden=../golden/test_number_to_compound_fraction.golden
python $test < $golden > actual
wc=`diff $golden actual | wc -l`
rm actual
if [ $wc = 0 ]
then
  echo "PASS", $test
else
  echo "*** FAIL ***", $test
fi

